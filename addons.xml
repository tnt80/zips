<addons>
<addon id="repository.tnt" name="TNT Repository" version="1.0.0" provider-name="TNT">
   <extension point="xbmc.addon.repository" name="TNT Repository">  
        <dir>
           <info compressed="false">https://raw.githubusercontent.com/tnt80/zips/master/addons.xml</info>
           <checksum>https://raw.githubusercontent.com/tnt80/zips/master/addons.xml.md5</checksum>
           <datadir zip="true">https://raw.githubusercontent.com/tnt80/zips/master/</datadir>
        </dir>		
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/jsergio123/zips/master/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/jsergio123/zips/master/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/jsergio123/zips/master/</datadir>
		</dir>		
	</extension>
		<extension point="xbmc.addon.metadata">
			<summary lang="en">TNT repo for Kodi</summary>
			<description lang="en">3th party Kodi plugins for the Dutch Kodi community</description>
			<platform>all</platform>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
			<screenshot>resources/screenshot_1.jpg</screenshot>
			<screenshot>resources/screenshot_2.jpg</screenshot>
			<screenshot>resources/screenshot_3.jpg</screenshot>
		</assets>
  </extension>
</addon>

<addon id="plugin.video.kindertubenl" name="KinderTube NL" version="1.1" provider-name="TGR">
	<requires>
		<import addon="script.module.audrey" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Voor de beste kinderfilmpjes ga je naar www.kindertube.nl</summary>
		<description>YouTube filmpjes voor kinderen van kindertube.nl. Special thanks to the dev. @ptom for the template addon!</description>
		<platform>all</platform>
		<language>en</language>
		<email></email>
	</extension>
</addon>

<addon id="plugin.video.kidstubenl" name="KidsTube NL" version="1.4.1" provider-name="TGR">
	<requires>
		<import addon="script.module.audrey" />
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Voor de beste kinderfilmpjes ga je naar www.kids-tube.nl</summary>
		<description>Kodi addon met YouTube filmpjes voor kinderen van kids-tube.nl. Special thanks to dev. @ptom for the template addon!</description>
		<platform>all</platform>
		<language>en</language>
		<email></email>
	</extension>
</addon>

<addon id="script.module.audrey" name="Audrey" version="1.1.7" provider-name="ptom">
	<requires>
        <import addon="script.module.urlresolver" />
        <import addon="script.module.resolveurl" />
        <import addon="script.module.requests" />
        <import addon="plugin.video.youtube" />
    </requires>
    <extension point="xbmc.python.module" library="lib" />
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary lang="en">Audrey - JSON file to Addon</summary>
        <description lang="en">Audrey takes a JSON feed (stored locally or online) and from this builds a Kodi addon</description>
        <disclaimer lang="en"></disclaimer>
        <language>en</language>
        <license>GNU GPL v.3</license>
        <forum>https://www.tvaddons.co/forums</forum>
		<website>https://www.tvaddons.co</website>
		<source>https://github.com/tvaddonsco/script.module.audrey</source>
    </extension>
</addon>

<addon id="script.module.urlresolver" name="URLResolver" version="5.0.16" provider-name="tvaddons">
	<requires>
		<import addon="xbmc.python" version="2.1.0" />
		<import addon="plugin.video.youtube" optional="true" />
		<import addon="script.module.python.twitch" optional="true" />
		<import addon="script.module.cryptopy" version="1.2.6" optional="true" />
		<import addon="plugin.video.gdrive" optional="true" />
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.python.pluginsource" library="lib/default.py" />
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Resolve common video host URL's to be playable in XBMC/Kodi.</summary>
		<summary lang="es">Resolver enlaces a videos de sitios populares para poder reproducirlos en XBMC/Kodi.</summary>
		<summary lang="hr">Prilagodi najčešće URL-ove video izvora kako bi radili u XBMC/Kodiju.</summary>
		<description lang="en">Resolve common video host URL's to be playable in XBMC/Kodi, simplify addon development of video plugins requiring multi video hosts.</description>
		<description lang="hr">Prilagodi najčešće URL-ove video izvora kako bi radili u XBMC/Kodiju, pojednostavnjuje razvoj priključaka za video dodatke koji zahtjevaju višestruke video izvore.</description>
		<forum>https://www.tvaddons.co/forums</forum>
        <website>https://www.tvaddons.co</website>
	</extension>
</addon>
</addons>